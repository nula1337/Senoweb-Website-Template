{
	"name": "senoweb",
	"private": true,
	"version": "1.0.0",
	"description": "Senoweb - ručně vyvinuté weby na míru",
	"scripts": {
		"watch:eleventy": "cross-env ELEVENTY_ENV=DEV eleventy --serve",
		"build:eleventy": "cross-env ELEVENTY_ENV=PROD eleventy",
		"watch:tailwind": "tailwindcss -i ./src/assets/css/input.css -o ./src/assets/css/main.css --watch --postcss",
		"build:tailwind": "tailwindcss -i ./src/assets/css/input.css -o ./src/assets/css/main.css --minify --postcss",
		"watch:cms": "npx decap-server",
		"start": "del-cli ./public --force && run-p watch:*",
		"build": "npm run build:tailwind && npm run build:eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nula1337/Senoweb-Website-Template"
	},
	"author": "Senoweb",
	"license": "ISC",
	"dependencies": {
		"decap-server": "^3.2.0",
		"del-cli": "^5.0.0",
		"netlify-plugin-cache": "^1.0.3"
	},
	"devDependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@codestitchofficial/eleventy-plugin-sharp-images": "^1.0.7",
		"@quasibit/eleventy-plugin-sitemap": "^2.2.0",
		"@sherby/eleventy-plugin-files-minifier": "^1.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"esbuild": "^0.25.0",
		"fluid-tailwind": "^1.0.4",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.5.2",
		"tailwind-scrollbar": "^3.1.0",
		"tailwindcss": "^3.4.17"
	}
}